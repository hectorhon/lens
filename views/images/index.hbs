<h1>Images</h1>
<p><a href="/images/upload" >Upload</a></p>
<div class="row">
    {{#each entries}}
        <div class="col-sm-3" >
            <a href="/images/view?id={{ this.id }}">
                <div>
                    <img class="img-fluid img-thumbnail rounded mx-auto d-block"
                         src="/images/get?id={{ this.id }}" />
                </div>
            </a>
        </div>
    {{/each}}
</div>
