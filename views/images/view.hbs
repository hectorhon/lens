<h1>View image</h1>
<p><a href="/images" >All images</a></p>
<div class="row">
    <div class="col-sm-6">
        <img class="img-fluid rounded mx-auto d-block"
             src="/images/get?id={{ imageId }}&fullsize=true" />
    </div>
    <div class="col-sm-6">
        <dl class="row">
            {{#each entry}}
                <dt class="col-sm-3">{{@key}}</dt>
                <dd class="col-sm-9">{{this}}</dd>
            {{/each}}
        </dl>
    </div>
</div>
